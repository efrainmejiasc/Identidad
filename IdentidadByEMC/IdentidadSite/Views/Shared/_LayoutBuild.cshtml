
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Identidad</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/font-awesome.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.6.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/modalStyle.css" />

    <!-- plugins:css -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/vendors/jquery-bar-rating/css-stars.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/vendors/font-awesome/css/font-awesome.min.css" />
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/assets/css/demo_1/style.css">
    <!-- End layout styles -->
</head>
<body class="sidebar-fixed" style="background-color: #F4F4F4; color: #1E2022 ">
    
    
    <div class="container-scroller">
        @Html.Partial("_sidebar")

            




        <div class="main-panel">
            @Html.Partial("_navbar")
            <div class="content-wrapper pb-0">
                @RenderBody()
            </div>
            @Html.Partial("_footer")
        </div>


        </div>
        <!-- Logout Modal-->
        <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Seguro de cerrar sesión?</h5>
                        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body">Selecciona salir sesión, en caso de estar seguro de finalizar su sesión</div>
                    <div class="modal-footer">
                        <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancelar</button>
                        <a class="btn btn-danger" href="~/Home/Logout">Salir</a>
                    </div>
                </div>
            </div>
        </div>
        <link rel="shortcut icon" href="~/lib/bootstrap/dist/css/assets/images/favicon.png" />



        <!-- plugins:js -->
        <script src="~/lib/bootstrap/dist/css/assets/vendors/js/vendor.bundle.base.js"></script>
        <!-- endinject -->
        <!-- Plugin js for this page -->
        <script src="~/lib/bootstrap/dist/css/assets/vendors/jquery-bar-rating/jquery.barrating.min.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/vendors/chart.js/Chart.min.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/vendors/flot/jquery.flot.js"></script>
        <script src="~/lib/bootstrap/dist/csst/assets/vendors/flot/jquery.flot.resize.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/vendors/flot/jquery.flot.categories.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/vendors/flot/jquery.flot.fillbetween.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/vendors/flot/jquery.flot.stack.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/js/jquery.cookie.js" type="text/javascript"></script>
        <!-- End plugin js for this page -->
        <!-- inject:js -->
        <script src="~/lib/bootstrap/dist/css/assets/js/off-canvas.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/js/hoverable-collapse.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/js/misc.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/js/settings.js"></script>
        <script src="~/lib/bootstrap/dist/css/assets/js/todolist.js"></script>
        <!-- endinject -->
        <!-- End custom js for this page -->


     <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

        @RenderSection("scripts", required: false)


</body>
</html>
